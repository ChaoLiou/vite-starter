{
  "name": "vite-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "build": "vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore \"src/**/*.{vue,ts,tsx}\"",
    "compile:ts": "vue-tsc --noEmit",
    "test": "yarn test:unit & yarn test:e2e",
    "test:unit": "jest --collect-coverage --maxWorkers=1",
    "test:e2e": "cd tests/e2e & yarn test:e2e",
    "test:e2e:alpha": "cd tests/e2e & yarn test:e2e:alpha",
    "cypress": "cd tests/e2e & yarn cypress",
    "cypress:alpha": "cd tests/e2e & yarn cypress:alpha",
    "postinstall": "husky install",
    "prepush": "yarn lint:script && yarn compile:ts && yarn test:unit"
  },
  "private": true,
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@antv/g2": "^4.1.32",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.23.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.29.1",
    "socket.io-client": "^4.3.2",
    "uuid": "^8.3.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-jest": "^27.2.5",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "miragejs": "^0.1.42",
    "prettier": "^2.4.1",
    "rollup-plugin-gzip": "^2.5.0",
    "sass": "^1.42.1",
    "ts-jest": "^27.0.6",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-style-import": "^1.2.1",
    "vue-tsc": "^0.3.0",
    "vue3-jest": "27.0.0-alpha.2"
  }
}
